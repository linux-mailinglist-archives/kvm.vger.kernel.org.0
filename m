> static void setup_fault() {
>  static struct {
>    const char* file;
>    const char* val;
>    bool fatal;
>  } files[] =3D {
>      {"/sys/kernel/debug/failslab/ignore-gfp-wait", "N", true},
>      {"/sys/kernel/debug/fail_futex/ignore-private", "N", false},
>      {"/sys/kernel/debug/fail_page_alloc/ignore-gfp-highmem", "N", false}=
