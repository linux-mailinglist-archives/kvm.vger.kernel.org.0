+++ b/target/i386/kvm/kvm.c
@@ -2221,6 +2221,7 @@ int kvm_arch_init_vcpu(CPUState *cs)
=20
             if (cs->kvm_state->xen_version >=3D XEN_VERSION(4, 17)) {
                 c->eax |=3D XEN_HVM_CPUID_UPCALL_VECTOR;
+                c->eax |=3D XEN_HVM_CPUID_EXT_DEST_ID;
             }
         }
=20
